<script src="/js/webflow.js" type="text/javascript"></script>
<script is:inline>
	document.addEventListener("DOMContentLoaded", () => {
		const form = document.querySelector("#contact-form");
		const subjectSelect = document.querySelector("#select-subject");
		const otherInput = document.querySelector("#input-other");
		const subjectFinal = document.querySelector("#subject-final");

		if (!form || !subjectSelect || !otherInput || !subjectFinal) return;

		// Show/hide Other-Subject
		const updateOtherVisibility = () => {
			const useOther = subjectSelect.value === "other";
			otherInput.hidden = !useOther;
			otherInput.disabled = !useOther;
			otherInput.required = useOther;
			if (!useOther) otherInput.value = "";
		};
		updateOtherVisibility();
		subjectSelect.addEventListener("change", updateOtherVisibility);

		// Map “Poptat …” buttons to select
		const subjectMap = [
			{ test: /vyrobu/i, value: "Zakázková výroba" }, // Poptat VÝROBU
			{ test: /vyvoj/i, value: "Zakázkový vývoj" }, // Poptat VÝVOJ
			{ test: /3D/i, value: "Velkoformátový 3D tisk" }, // Poptat Tisk
            { test: /FDM/i, value: "Zakázkový 3D tisk" }, // Poptat FDM Tisk
            { test: /SLA/i, value: "SLA tisk" }, // Poptat SLA tisk
            { test: /UV/i, value: "UV potisk" }, // Poptat UV Potisk
		];
		const stripAccents = (s) =>
			s.normalize("NFD").replace(/[\u0300-\u036f]/g, "");

		document.querySelectorAll('a[href="#contact"]').forEach((a) => {
			a.addEventListener(
				"click",
				() => {
					const labelEl = a.querySelector(".primary-button-text");
					const label = (
						labelEl ? labelEl.textContent : a.textContent
					).trim();
					if (!/poptat/i.test(label)) return;

					const raw = stripAccents(label).toLowerCase();
					const match = subjectMap.find((m) => m.test.test(raw));
					if (!match) return;

					const opt = Array.from(subjectSelect.options).find(
						(o) => o.value === match.value,
					);
					if (opt) {
						subjectSelect.value = opt.value;
						subjectSelect.dispatchEvent(
							new Event("change", { bubbles: true }),
						);
					}
				},
				{ passive: true },
			);
		});

		// Normalize subject for Netlify (single canonical field)
		form.addEventListener("submit", () => {
			const isOther = subjectSelect.value === "other";
			subjectFinal.value = isOther
				? otherInput.value || "Vlastní"
				: subjectSelect.value;
		});
	});
</script>
<form name="contact-form" netlify netlify-honeypot="bot-field" hidden>
	<input type="text" name="First-Name" />
	<input type="text" name="Last-Name" />
	<input type="email" name="E-Mail" />
	<textarea name="Message"></textarea>
	<input type="text" name="Subject-Final" />
	<input name="bot-field" />
</form>